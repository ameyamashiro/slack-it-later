{
    "manifest_version": 2,

    "name": "Post to slack",
    "description": "This extension posts an url which you are watching to a slack channel.",
    "version": "0.1",

    "permissions": [
        "tabs"
    ],

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "options_page": "options.html"
}
